md-list
  md-list-item.md-3-line.md-whiteframe-1dp(ng-repeat="flag in main.feed")
    img(ng-src="{{flag.snippet.thumbnails.default.url}}" ui-sref="detail({id: flag.id})")

    .md-list-item-text
      h3(ui-sref="detail({id: flag.id})") {{flag.snippet.title}}
      p(hide-xs) {{flag.snippet.description}}

paging(
  ng-init="currentPage=1"
  page-size="5"
  page="currentPage"
  total="2"
)